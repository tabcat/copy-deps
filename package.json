{
  "name": "dep-sync",
  "version": "1.0.0",
  "description": "keep dependencies equal with those of another package",
  "type": "module",
  "main": "./dist/src/index.js",
  "bin": {
    "dep-sync": "./bin/dep-sync.js"
  },
  "files": [
    "./bin",
    "./dist/src"
  ],
  "scripts": {
    "lint": "eslint src",
    "format": "prettier -w src && eslint --fix src",
    "build": "tsc && cp ./dist/src/index.js ./bin/dep-sync.js",
    "prepublishOnly": "npm run build",
    "test": "c8 -c ./config/.c8rc.json npx mocha --config ./config/.mocharc.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tabcat/dep-sync.git"
  },
  "author": "tabcat <tabcat00@proton.me>",
  "license": "(MIT OR Apache-2.0)",
  "bugs": {
    "url": "https://github.com/tabcat/dep-sync/issues"
  },
  "homepage": "https://github.com/tabcat/dep-sync",
  "devDependencies": {
    "@types/mocha": "^10.0.0",
    "c8": "^7.12.0",
    "eslint": "^8.26.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "mocha": "^10.1.0",
    "prettier": "^2.7.1",
    "prettier-config-standard": "^5.0.0",
    "resolve-typescript-plugin": "^1.2.0",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "commander": "^9.4.1"
  }
}
